{
  "expo": {
    "name": "LogBook",
    "slug": "LogBook",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/LogBook_icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/LogBook_icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "Aplikácia potrebuje prístup ku galérii, aby ste mohli pridať fotky do svojich záznamov.",
        "ITSAppUsesNonExemptEncryption": false
      },
      "bundleIdentifier": "com.matejcik24.LogBook"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.matejcik24.LogBook"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-image-picker",
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "a8378b14-3bfe-4ae8-abaa-f5ee04d6839c"
      }
    }
  }
}
